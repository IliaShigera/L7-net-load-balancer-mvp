{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "LoadBalancer": "Information"
    }
  },
  "HealthCheck": {
    "Path": "/health",
    "IntervalSeconds": 5,
    "TimeoutSeconds": 2
  },
  "Instances": [
    {
      "Name": "instanceA",
      "Address": "http://localhost:5001"
    },
    {
      "Name": "instanceB",
      "Address": "http://localhost:5002"
    }
  ]
}